                    .property__plan-prev
                      +icon('ico-arrow-left')
                    .property__plan-slider.swiper-container(js-slider-plan)
                      .swiper-wrapper
                        .swiper-slide
                          .property__plan-wrap
                            img(src='img/propertyFloor_1.png' srcset='img/propertyFloor_1@2x.png 2x')
                            span первый этаж
                        .swiper-slide
                          .property__plan-wrap
                            img(src='img/propertyFloor_1.png' srcset='img/propertyFloor_1@2x.png 2x')
                            span второй этаж
                        .swiper-slide
                          .property__plan-wrap
                            img(src='img/propertyFloor_1.png' srcset='img/propertyFloor_1@2x.png 2x')
                            span третьий этаж
                        .swiper-slide
                          .property__plan-wrap
                            img(src='img/propertyFloor_1.png' srcset='img/propertyFloor_1@2x.png 2x')
                            span четвертый этаж
                                
                    
          // section
          .property__section
            h3.property__heading Расположение 
            .property__tab(data-tab="property_map").is-active
              .property__tab-mobile
                span Расположение
                +icon('ico-arrow-right')
                
              .property__info
                .property__map
                  #property-map
                  // set params for print map
                  .property__map-static
                    img(src="https://static-maps.yandex.ru/1.x/?ll=37.63999999999997,55.75999999999371&size=640,250&z=12&l=map&pt=37.63999999999997,55.75999999999371,pmwtm1")
                
