                    img(src='img/galleryMain.jpg' srcset='img/galleryMain@2x.jpg 2x')
                .gallery__main-slide
                  a(href="img/galleryMain@2x.jpg").no-barba
                    img(src='img/galleryMain.jpg' srcset='img/galleryMain@2x.jpg 2x')
                .gallery__main-slide
                  a(href="img/galleryMain@2x.jpg").no-barba
                    img(src='img/galleryMain.jpg' srcset='img/galleryMain@2x.jpg 2x')
